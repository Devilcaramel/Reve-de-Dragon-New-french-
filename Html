        <td>Points d'endurance</td><td style="padding-left:10px;"><input type="number" name="attr_pts_endurance_max" class="bulle" readonly="true"  </td><td  style="padding-left:20px"><input type="number" name="attr_pts_endurance" class="bulle" readonly="true" ></td><td>/</td><td><input type="number" name="attr_pts_endurance_max" readonly="true"></td>
        <td><button type="action" name="act_endurance_plus">+1</button>  &nbsp;   <button type="action" name="act_endurance_minus">-1</button> <button  type="roll" style="margin-left:0px;line_height:10px;height:20px;margin-bottom:5px;" title="Jet d'endurance" name="roll_Endujet" value="&{template:jetendu} {{name=Jet d'endurance}} {{Roll=[[1d20]]}} {{endu=[[@{pts_endurance}]]}}">Jet d'endurance</button>
        <input type="hidden" name="attr_pts_endurance" value="100"/>
        </td>
