        <td>Points de Vie</td><td style="padding-left:10px;"><input type="number" name="attr_pts_vie_max" class="bulle" readonly="true"></td><td style="padding-left:20px"<td  style="padding-left:20px"><input type="number" name="attr_pts_vie" class="bulle" readonly="true"  ></td><td>/</td><td><input type="number" name="attr_pts_vie_max" readonly="true" ></td>
         <td><button type="action" name="act_vie_plus">+1</button> &nbsp; <button type="action" name="act_vie_minus">-1</button><button  type="roll" style="margin-left:0px;line_height:10px;height:20px;margin-bottom:5px;" title="Jet de vie" name="roll_viejet" value="&{template:life} {{name=Jet de vie}} {{Roll=[[1d20]]}} {{life=[[@{pts_vie}]]}} {{const1=[[@{seuil_constitution}]]}}">Jet de vie</button></td>
