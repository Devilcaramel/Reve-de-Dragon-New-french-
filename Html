           <td>Echec total</td>
           <input type="number" name="attr_fumble" value="ceil(100-(100-floor((((@{rollDiff}+@{abilityRoll}+@{etat_general}+@{Visée}+@{modRoll}+@{moralbonus})/10)+1)*(@{attributeRoll}*5)))*0.1)+1" disabled="true"  style="width:50px"/>
            <div  style="width:100px">
                <button type='roll' name='roll_CustomRollPublic' style="font-size:12px"  value="&{template:RDD} {{name=@{rollTitle}}} {{moralbonus=@{moralbonus}}} {{Roll=[[1D100cs<0cf>100]]}} {{dif=[[@{rollDiff}]]}} {{difficulte=[[@{rollDiff}+@{abilityRoll}+@{etat_general}+@{Visée}+@{modRoll}+@{moralbonus}]]}}  {{attribut=[[@{attributeRoll}}]]}} {{echec_part=[[ceil(0.2*floor((((@{rollDiff}+@{abilityRoll}+@{etat_general}+@{Visée}+@{modRoll}+@{moralbonus})/10)+1)*(@{attributeRoll}*5)))+80]]}}  {{signif=[[floor(0.5*floor((((@{rollDiff}+@{abilityRoll}+@{etat_general}+@{Visée}+@{modRoll}+@{moralbonus})/10)+1)*(@{attributeRoll}*5)))]]}} {{reussite_par=[[ceil(0.2*floor((((@{rollDiff}+@{abilityRoll}+@{etat_general}+@{Visée}+@{modRoll}+@{moralbonus})/10)+1)*(@{attributeRoll}*5)))]]}} {{cible=[[floor((((@{rollDiff}+@{abilityRoll}+@{etat_general}+@{Visée}+@{modRoll}+@{moralbonus})/10)+1)*(@{attributeRoll}*5))]]}} {{echec_tot=[[ceil(100-(100-floor((((@{rollDiff}+@{abilityRoll}+@{etat_general}+@{Visée}+@{modRoll}+@{moralbonus})/10)+1)*(@{attributeRoll}*5)))*0.1)+1]]}} {{cible9=[[floor(@{attributeRoll}/2)]]}} {{echect9=[[92]]}} {{echect8=[[floor(@{attributeRoll}*0.099)+92]]}} {{parti8=[[ceil(@{attributeRoll}/5)]]}} {{parti9=[[floor(@{attributeRoll}/6)]]}}  {{cible10=[[floor(@{attributeRoll}/4)]]}} {{echect10=[[92]]}} {{parti10=[[1]]}} {{parti11=[[0]]}} {{cible11=[[1]]}} {{cible12=[[1]]}} {{cible13=[[1]]}} {{cible14=[[1]]}} {{cible15=[[1]]}} {{cible16=[[1]]}}  {{echect11=[[90]]}} {{echect12=[[70]]}} {{echect13=[[50]]}} {{echect14=[[30]]}} {{echect15=[[10]]}} {{echect16=[[02]]}} {{echect17=[[01]]}} {{cible17=[[0]]}}">public</button></td>
 <button type='roll' name='roll_CustomRollPrivate' style="font-size:12px" value='/w gm &{template:RDD} {{name=@{rollTitle}}} {{moralbonus=@{moralbonus}}} {{Roll=[[1D100cs<0cf>100]]}} {{dif=[[@{rollDiff}]]}} {{difficulte=[[@{rollDiff}+@{abilityRoll}+@{etat_general}+@{Visée}+@{modRoll}+@{moralbonus}]]}}  {{attribut=[[@{attributeRoll}}]]}} {{echec_part=[[ceil(0.2*floor((((@{rollDiff}+@{abilityRoll}+@{etat_general}+@{Visée}+@{modRoll}+@{moralbonus})/10)+1)*(@{attributeRoll}*5)))+80]]}}  {{signif=[[floor(0.5*floor((((@{rollDiff}+@{abilityRoll}+@{etat_general}+@{Visée}+@{modRoll}+@{moralbonus})/10)+1)*(@{attributeRoll}*5)))]]}} {{reussite_par=[[ceil(0.2*floor((((@{rollDiff}+@{abilityRoll}+@{etat_general}+@{Visée}+@{modRoll}+@{moralbonus})/10)+1)*(@{attributeRoll}*5)))]]}} {{cible=[[floor((((@{rollDiff}+@{abilityRoll}+@{etat_general}+@{Visée}+@{modRoll}+@{moralbonus})/10)+1)*(@{attributeRoll}*5))]]}} {{echec_tot=[[ceil(100-(100-floor((((@{rollDiff}+@{abilityRoll}+@{etat_general}+@{Visée}+@{modRoll}+@{moralbonus})/10)+1)*(@{attributeRoll}*5)))*0.1)+1]]}} {{cible9=[[floor(@{attributeRoll}/2)]]}} {{echect9=[[92]]}} {{echect8=[[floor(@{attributeRoll}*0.099)+92]]}} {{parti8=[[ceil(@{attributeRoll}/5)]]}} {{parti9=[[floor(@{attributeRoll}/6)]]}}  {{cible10=[[floor(@{attributeRoll}/4)]]}} {{echect10=[[92]]}} {{parti10=[[1]]}} {{parti11=[[0]]}} {{cible11=[[1]]}} {{cible12=[[1]]}} {{cible13=[[1]]}} {{cible14=[[1]]}} {{cible15=[[1]]}} {{cible16=[[1]]}}  {{echect11=[[90]]}} {{echect12=[[70]]}} {{echect13=[[50]]}} {{echect14=[[30]]}} {{echect15=[[10]]}} {{echect16=[[02]]}} {{echect17=[[01]]}} {{cible17=[[0]]}}'> Private</button>

