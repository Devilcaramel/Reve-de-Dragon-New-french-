<script type="text/worker">

on("change:fatigue_ajustement change:ethyl change:pts_vie change:pts_vie_max", function(){
        getAttrs(["fatigue_ajustement","ethyl", "etat_general", "pts_vie", "pts_vie_max"], function(values){
           setAttrs({"etat_general":-(values.pts_vie_max-values.pts_vie)+values.fatigue_ajustement-values.ethyl});
        });
    });
/*
 *  Fonctions dans l'ordre d'apparition
 */

/* Poids commentaire cf. p46 */
function setPoidsComment() {
  getAttrs(["taille"], function(res) {
    var com;
    switch (parseInt(res.taille)) {
    case 0: com = '0'; break;
    case 1: com = 'moins de 1'; break;
    case 2: com = '1-5 kg'; break;
    case 3: com = '6-10 kg'; break;
    case 4: com = '11-20 kg'; break;
    case 5: com = '21-30 kg'; break;
    case 6: com = '31-40 kg'; break;
    case 7: com = '41-50 kg'; break;
    case 8: com = '51-60 kg'; break;
    case 9: com = '61-65 kg'; break;
    case 10: com = '66-70 kg'; break;
    case 11: com = '71-75 kg'; break;
    case 12: com = '76-80 kg'; break;
    case 13: com = '81-90 kg'; break;
    case 14: com = '91-100 kg'; break;
    case 15: com = '101-110 kg'; break;
    case 16: com = '111-120 kg'; break;
    case 17: com = '121-130 kg'; break;
    case 18: com = '131-140 kg'; break;
    case 19: com = '141-150 kg'; break;
    case 20: com = '151-160 kg'; break;
    case 21: com = '161-180 kg'; break;
    case 22: com = '181-200 kg'; break;
    case 23: com = '201-300 kg'; break;
    case 24: com = '301-400 kg'; break;
    case 25: com = '401-500 kg'; break;
    case 26: com = '501-600 kg'; break;
    case 27: com = '601-700 kg'; break;
    case 28: com = '701-800 kg'; break;
    case 29: com = '801-900 kg'; break;
    case 30: com = '901-1000 kg'; break;
    case 31: com = '1000-1500 kg'; break;
    case 32: com = '1501-2000 kg'; break;
    default: com = '2001+ kg';
    }
    setAttrs({info_poids_comment: com});
  });
}

/* Force max = Taille + 4 */
function setCaracForceMax() {
  getAttrs(["taille"], function(res) {
    setAttrs({force_value_max: parseInt(res.taille) + 4});
  });
}

/* Mêlée = Force + Agilité / 2 */
function setCaracMelee() {
  getAttrs(["force_value", "agilite_value"], function(res) {
    var melee = Math.floor((parseInt(res.force_value) + parseInt(res.agilite_value)) / 2);
    setAttrs({melee_value: melee});
  });
}

/* Tir = Vue + Dextérité / 2 */
function setCaracTir() {
  getAttrs(["vue_value", "dexterite_value"], function(res) {
    var tir = Math.floor((parseInt(res.vue_value) + parseInt(res.dexterite_value)) / 2);
    setAttrs({tir_value: tir});
  });
}

/* Lancer = Tir + Force / 2 */
function setCaracLancer() {
  getAttrs(["vue_value", "dexterite_value", "force_value"], function(res) {
    var tir = Math.floor((parseInt(res.vue_value) + parseInt(res.dexterite_value)) / 2);
    var lancer = Math.floor((tir + parseInt(res.force_value)) / 2);
    setAttrs({lancer_value: lancer});
  });
}

/* Dérobée = Agilité + (21 - Taille) / 2 */
function setCaracDerobee() {
  getAttrs(["agilite_value", "taille"], function(res) {
    var derobee = Math.floor((parseInt(res.agilite_value) + (21 - parseInt(res.taille))) / 2);
    setAttrs({derobee_value: derobee});
  });
}

/* Pts de vie max = Taille + Constitution / 2 */
function setPtsVieMax() {
  getAttrs(["taille", "constitution_value", "pts_vie"], function(res) {
    var pvmax = Math.ceil((parseInt(res.taille) + parseInt(res.constitution_value)) / 2);
    var pv = parseInt(res.pts_vie);
    if (pv > pvmax) {
      pv = pvmax;
    }
    setAttrs({pts_vie_max: pvmax, pts_vie: pv});
  });
}

/* Endurance max = Taille + Const OU Pts de vie + Volonté  */
function setPtsEnduranceMax() {
  getAttrs(["taille", "constitution_value", "volonte_value", "pts_endurance"], function(res) {
    var end1 = parseInt(res.taille) + parseInt(res.constitution_value);
    var pv = Math.ceil((parseInt(res.taille) + parseInt(res.constitution_value)) / 2);
    var end2 = pv + parseInt(res.volonte_value);
    var enduranceMax = end1 >= end2 ? end1 : end2;
    var ptsEndurance = parseInt(res.pts_endurance);
    if (ptsEndurance > enduranceMax) {
      ptsEndurance = enduranceMax;
    }
    setAttrs({pts_endurance_max: enduranceMax, pts_endurance: ptsEndurance});
  });
}

/* Seuil de Constitution cf. p381 */
function setSeuilConstitution() {
  getAttrs(["constitution_value"], function(res) {
    var constit = parseInt(res.constitution_value);
    var sc;
    switch (constit) {
    case 0: sc = 0; break;
    case 1:
    case 2: sc = 0.5; break;
    default: sc = Math.floor(constit / 3);
    }
    setAttrs({seuil_constitution: sc});
  });
}

/* Seuil de sustentation cf. p381 */
function setSeuilSustentation() {
  getAttrs(["taille"], function(res) {
    var tai = parseInt(res.taille);
    var sust;
    switch (tai) {
    case 0: sust = 0; break;
    case 1: sust = 0.1; break;
    case 2: sust = 0.3; break;
    case 3: sust = 0.5; break;
    case 4:
    case 5: sust = 1; break;
    case 6:
    case 7:
    case 8:
    case 9: sust = 2; break;
    case 10:
    case 11:
    case 12:
    case 13: sust = 3; break;
    case 14:
    case 15:
    case 16: sust = 4; break;
    case 17:
    case 18:
    case 19: sust = 5; break;
    case 20:
    case 21: sust = 6; break;
    default: sust = 7 + (tai - 22);
    }
    setAttrs({seuil_sustentation: sust});
  });
}

/* Bonus aux dommages cf. p45 p381 */
function setBonusDommages() {
    getAttrs(["taille", "force_value"], function(res) {
        var moy = Math.floor((parseInt(res.taille) + parseInt(res.force_value)) / 2);
        var dom;
        switch (moy) {
        case 0:
            dom = -6; break;
        case 1:
            dom = -5; break;
        case 2:
            dom = -4; break;
        case 3: case 4:
            dom = -3; break;
        case 5:
            dom = -2; break;
        case 6: case 7:
            dom = -1; break;
        case 8: case 9: case 10: case 11:
            dom = 0; break;
        case 12: case 13:
            dom = 1; break;
        case 14: case 15:
            dom = 2; break;
        case 16: case 17:
            dom = 3; break;
        case 18: case 19: case 20:
            dom = 4; break;
        case 21: case 22:
            dom = 5; break;
        case 23: case 24:
            dom = 6; break;
        case 25: case 26:
            dom = 7; break;
        case 27: case 28:
            dom = 8; break;
        case 29: case 30:
            dom = 9; break;
        case 31:
            dom = 10; break;
        case 32:
            dom = 11; break;
        default:
            dom = 12 + (moy - 33);
        }
        setAttrs({bonus_dommages: dom});
    });
}

/* Seuil d'encombrement cf. p45 */
function setSeuilEncombrement() {
    getAttrs(["taille", "force_value"], function(res) {
        var enc = (parseInt(res.taille) + parseInt(res.force_value)) / 2;
        setAttrs({seuil_encombrement: enc});
    });
}

/* Caractéristiques total de points */
function setCaracTotal() {
    getAttrs(["carac_beaute", "taille", "apparence_value", "constitution_value", "force_value",
        "agilite_value", "dexterite_value", "vue_value", "ouie_value", "odoratgout_value",
        "volonte_value", "intellect_value", "empathie_value", "reve_value", "chance_value"],
    function(res) {
        var tot = 0;
        tot += parseInt(res.carac_beaute) > 10 ? parseInt(res.carac_beaute) - 10 : 0;
        tot += parseInt(res.taille); tot += parseInt(res.apparence_value); tot += parseInt(res.constitution_value);
        tot += parseInt(res.force_value); tot += parseInt(res.agilite_value); tot += parseInt(res.dexterite_value);
        tot += parseInt(res.vue_value); tot += parseInt(res.ouie_value); tot += parseInt(res.odoratgout_value);
        tot += parseInt(res.volonte_value); tot += parseInt(res.intellect_value); tot += parseInt(res.empathie_value);
        tot += parseInt(res.reve_value); tot += parseInt(res.chance_value);
        setAttrs({carac_total: tot});
    });
}



/* Fatigue, cases à cocher */
function setFatigueCases() {
  getAttrs(['pts_fatigue', 'pts_fatigue_max',
    'fatigue_segment_01_max', 'fatigue_segment_02_max', 'fatigue_segment_03_max',
    'fatigue_segment_04_max', 'fatigue_segment_05_max', 'fatigue_segment_06_max',
    'fatigue_segment_07_max', 'fatigue_segment_08_max', 'fatigue_segment_09_max',
    'fatigue_segment_10_max', 'fatigue_segment_11_max', 'fatigue_segment_12_max'],
  function(res) {
    var ptsFatigue = parseInt(res.pts_fatigue);
    var fatigueMax = parseInt(res.pts_fatigue_max);
    var segmax = [];
    segmax[0] = parseInt(res.fatigue_segment_01_max);
    segmax[1] = parseInt(res.fatigue_segment_02_max);
    segmax[2] = parseInt(res.fatigue_segment_03_max);
    segmax[3] = parseInt(res.fatigue_segment_04_max);
    segmax[4] = parseInt(res.fatigue_segment_05_max);
    segmax[5] = parseInt(res.fatigue_segment_06_max);
    segmax[6] = parseInt(res.fatigue_segment_07_max);
    segmax[7] = parseInt(res.fatigue_segment_08_max);
    segmax[8] = parseInt(res.fatigue_segment_09_max);
    segmax[9] = parseInt(res.fatigue_segment_10_max);
    segmax[10] = parseInt(res.fatigue_segment_11_max);
    segmax[11] = parseInt(res.fatigue_segment_12_max);
    var cases = [];
    var coches = 0;
    var ajustement = 0;
    var statut = '';
    var i, j;
    for (i = 0 ; i < 12; i++) {
      for (j = 0; j < 8; j++) {
        if (j + 1 > segmax[i]) {
          cases[(i * 8) + j] = '';
        }
        else {
          if (coches < ptsFatigue) {
            cases[(i * 8) + j] = 'X';
            coches++;
            if (coches == ptsFatigue) {
              switch (i) {
              case 0: case 1: case 2:
                ajustement = 0; break;
              case 3: case 4: case 5:
                ajustement = -1; break;
              case 6:
                ajustement = -2; break;
              case 7:
                ajustement = -3; break;
              case 8:
                ajustement = -4; break;
              case 9:
                ajustement = -5; break;
              case 10:
                ajustement = -6; break;
              case 11:
              default:
                ajustement = -7;
                if (ptsFatigue == fatigueMax) {
                  statut = 'Épuisé! Endormi une heure!';
                }
                break;
              }
            }
          }
          else {
            cases[(i * 8) + j] = 'O';
          }
        }
      }
    }
    setAttrs({fatigue_ajustement: ajustement, fatigue_statut: statut,
      fatigue_case_01: cases[0], fatigue_case_02: cases[1], fatigue_case_03: cases[2], fatigue_case_04: cases[3],
      fatigue_case_05: cases[4], fatigue_case_06: cases[5], fatigue_case_07: cases[6], fatigue_case_08: cases[7],
      fatigue_case_09: cases[8], fatigue_case_10: cases[9], fatigue_case_11: cases[10], fatigue_case_12: cases[11],
      fatigue_case_13: cases[12], fatigue_case_14: cases[13], fatigue_case_15: cases[14], fatigue_case_16: cases[15],
      fatigue_case_17: cases[16], fatigue_case_18: cases[17], fatigue_case_19: cases[18], fatigue_case_20: cases[19],
      fatigue_case_21: cases[20], fatigue_case_22: cases[21], fatigue_case_23: cases[22], fatigue_case_24: cases[23],
      fatigue_case_25: cases[24], fatigue_case_26: cases[25], fatigue_case_27: cases[26], fatigue_case_28: cases[27],
      fatigue_case_29: cases[28], fatigue_case_30: cases[29], fatigue_case_31: cases[30], fatigue_case_32: cases[31],
      fatigue_case_33: cases[32], fatigue_case_34: cases[33], fatigue_case_35: cases[34], fatigue_case_36: cases[35],
      fatigue_case_37: cases[36], fatigue_case_38: cases[37], fatigue_case_39: cases[38], fatigue_case_40: cases[39],
      fatigue_case_41: cases[40], fatigue_case_42: cases[41], fatigue_case_43: cases[42], fatigue_case_44: cases[43],
      fatigue_case_45: cases[44], fatigue_case_46: cases[45], fatigue_case_47: cases[46], fatigue_case_48: cases[47],
      fatigue_case_49: cases[48], fatigue_case_50: cases[49], fatigue_case_51: cases[50], fatigue_case_52: cases[51],
      fatigue_case_53: cases[52], fatigue_case_54: cases[53], fatigue_case_55: cases[54], fatigue_case_56: cases[55],
      fatigue_case_57: cases[56], fatigue_case_58: cases[57], fatigue_case_59: cases[58], fatigue_case_60: cases[59],
      fatigue_case_61: cases[60], fatigue_case_62: cases[61], fatigue_case_63: cases[62], fatigue_case_64: cases[63],
      fatigue_case_65: cases[64], fatigue_case_66: cases[65], fatigue_case_67: cases[66], fatigue_case_68: cases[67],
      fatigue_case_69: cases[68], fatigue_case_70: cases[69], fatigue_case_71: cases[70], fatigue_case_72: cases[71],
      fatigue_case_73: cases[72], fatigue_case_74: cases[73], fatigue_case_75: cases[74], fatigue_case_76: cases[75],
      fatigue_case_77: cases[76], fatigue_case_78: cases[77], fatigue_case_79: cases[78], fatigue_case_80: cases[79],
      fatigue_case_81: cases[80], fatigue_case_82: cases[81], fatigue_case_83: cases[82], fatigue_case_84: cases[83],
      fatigue_case_85: cases[84], fatigue_case_86: cases[85], fatigue_case_87: cases[86], fatigue_case_88: cases[87],
      fatigue_case_89: cases[88], fatigue_case_90: cases[89], fatigue_case_91: cases[90], fatigue_case_92: cases[91],
      fatigue_case_93: cases[92], fatigue_case_94: cases[93], fatigue_case_95: cases[94], fatigue_case_96: cases[95]});
  });
}

/* Fatigue max = endurance max * 2, répartition des segments */
function setFatigueMax() {
  getAttrs(['pts_endurance_max', 'pts_fatigue'], function(res) {
    var enduranceMax = parseInt(res.pts_endurance_max);
    var fatigueMax = enduranceMax * 2;
    var ptsFatigue = parseInt(res.pts_fatigue) > fatigueMax ? fatigueMax : parseInt(res.pts_fatigue);
    var seg1, seg2, seg3, seg4, seg5, seg6, seg7, seg8, seg9, seg10, seg11, seg12;
    seg1 = seg7 = enduranceMax - 5 <= 0 ? 0 : Math.ceil((enduranceMax - 5) / 6);
    seg2 = seg8  = enduranceMax -3 <= 0 ? 0 : Math.ceil((enduranceMax - 3) / 6);
    seg3 = seg9  = enduranceMax - 1 <= 0 ? 0 : Math.ceil((enduranceMax - 1) / 6);
    seg4 = seg10 = enduranceMax - 4 <= 0 ? 0 : Math.ceil((enduranceMax - 4) / 6);
    seg5 = seg11 = enduranceMax - 2 <= 0 ? 0 : Math.ceil((enduranceMax - 2) / 6);
    seg6 = seg12 = enduranceMax <= 0 ? 0 : Math.ceil(enduranceMax / 6);
    setAttrs({pts_fatigue_max: fatigueMax, pts_fatigue: ptsFatigue,
      fatigue_segment_01_max: seg1, fatigue_segment_02_max: seg2, fatigue_segment_03_max: seg3,
      fatigue_segment_04_max: seg4, fatigue_segment_05_max: seg5, fatigue_segment_06_max: seg6,
      fatigue_segment_07_max: seg7, fatigue_segment_08_max: seg8, fatigue_segment_09_max: seg9,
      fatigue_segment_10_max: seg10, fatigue_segment_11_max: seg11, fatigue_segment_12_max: seg12}, {}, setFatigueCases);
  });
}

/* Fatigue +x */
function fatiguePlus(cnt) {
  getAttrs(['pts_fatigue', 'pts_fatigue_max'], function(res) {
    var i;
    var ptsFatigue = parseInt(res.pts_fatigue);
    var fatigueMax = parseInt(res.pts_fatigue_max);
    for (i = 0; i < cnt; i++) {
      if (ptsFatigue + 1 <= fatigueMax) {
        ++ptsFatigue;
      }
      else {
        break;
      }
    }
    setAttrs({pts_fatigue: ptsFatigue});
  });
}

/* Fatigue -x */
function fatigueMinus(cnt) {
  getAttrs(['pts_fatigue', 'pts_endurance', 'pts_endurance_max'], function(res) {
    var i;
    var ptsFatigue = parseInt(res.pts_fatigue);
    var ptsEndurance = parseInt(res.pts_endurance);
    var enduranceMax = parseInt(res.pts_endurance_max);
    var ptsPerdus = enduranceMax - ptsEndurance;
    for (i = 0; i < cnt; i++) {
      if (ptsFatigue - 1 >= 0 && ptsFatigue - 1 >= ptsPerdus) {
        ptsFatigue--;
      }
    }
    setAttrs({pts_fatigue: ptsFatigue});
  });
}

/* Endurance -x */
function enduranceMinus(cnt) {
  getAttrs(['pts_endurance'], function(res) {
    var i, num = 0;
    var ptsEndurance = parseInt(res.pts_endurance);
    for (i = 0; i < cnt; i++) {
      if (ptsEndurance - 1 >= 0) {
        --ptsEndurance;
        ++num;
      }
      else {
        break;
      }
    }
    if (num) {
      fatiguePlus(num);
    }
    if (ptsEndurance == 0) {
      vieMinus(1);
      setAttrs({pts_endurance: ptsEndurance, endurance_statut: 'Évanoui!'});
    }
    else {
      setAttrs({pts_endurance: ptsEndurance, endurance_statut: ''});
    }
  });
}

/* Pts de Vie -x */
function vieMinus(cnt) {
  getAttrs(['pts_vie', 'seuil_constitution', 'pts_vie_max'], function(res) {
    var i;
    var pv = parseInt(res.pts_vie);
    var sc = parseInt(res.seuil_constitution);
    var pvmax = parseInt(res.pts_vie_max);
    var dead = (-sc) - 1;
    var statut = '';
    for (i = 0; i < cnt; i++) {
      if (pv - 1 >= dead) {
        pv--;
      }
    }
    if (pv == dead) {
      statut = 'Mort!';
    }
    else if (pv <= 0) {
      statut = 'Coma!';
    }
    setAttrs({pts_vie: pv, vie_statut: statut, pts_vie_perdus: -(pvmax - pv),
      pts_vie_perdus_exp: Math.floor((pvmax - pv) / 2)});
  });
}





/*
 *  Ouverture de la fiche
 */
on("sheet:opened", function() {
  /* Poids, commentaire */
  setPoidsComment();
  /* Force max = Taille + 4 */
  setCaracForceMax();
  /* Mêlée = Force + Agilité / 2 */
  setCaracMelee();
  /* Tir = Vue + Dextérité / 2 */
  setCaracTir();
  /* Lancer = Tir + Force / 2 */
  setCaracLancer();
  /* Dérobée = Agilité + (21 - Taille) / 2 */
  setCaracDerobee();
  /* Pts de vie max = Taille + Constitution / 2 */
  setPtsVieMax();
  /* Pts d'endurance max = Taille + Const OU Pts de vie + Volonté */
  setPtsEnduranceMax();
  /* Seuil de Constitution */
  setSeuilConstitution();
  /* Seuil de sustentation */
  setSeuilSustentation();
  /* Bonus aux dommages */
  setBonusDommages();
  /* Seuil d'encombrement */
  setSeuilEncombrement();
  /* Caractéristiques, total de points */
  setCaracTotal();
  /* Fatigue max, segments et cases */
  setFatigueMax();
});






/*
 *  Triggers
 */

on("change:taille", function(ev) {
    setPoidsComment();
});


/* Force max = Taille + 4 */
on("change:taille", function(ev) {
    var force_max = parseInt(ev.newValue) + 4;
    setAttrs({force_value_max: force_max});
    getAttrs(["force_value"], function(res) {
        if (parseInt(res.force_value) > force_max) {
            setAttrs({force_value: force_max});
        }
    });
});
on("change:force_value", function(ev) {
    if (ev.sourceType == "sheetworker") return;
    getAttrs(["force_value_max"], function(res) {
        if (parseInt(ev.newValue) > parseInt(res.force_value_max)) {
            setAttrs({force_value: parseInt(res.force_value_max)});
        }
    });
});

/* Mêlée = Force + Agilité / 2 */
on("change:force_value change:agilite_value", function(ev) {
    setCaracMelee();
});

/* Tir = Vue + Dextérité / 2 */
on("change:vue_value change:dexterite_value", function(ev) {
    setCaracTir();
});

/* Lancer = Tir + Force / 2 */
on("change:vue_value change:dexterite_value change:force_value", function(ev) {
    setCaracLancer();
});

/* Dérobée = Agilité + (21 - Taille) / 2 */
on("change:agilite_value change:taille", function(ev) {
    setCaracDerobee();
});

/* Caractéristiques total de points */
on("change:carac_beaute change:taille change:apparence_value change:constitution_value \
    change:force_value change:agilite_value change:dexterite_value change:vue_value \
    change:ouie_value change:odoratgout_value change:volonte_value change:intellect_value \
    change:empathie_value change:reve_value change:chance_value", function(ev) {
    setCaracTotal();
});

/* Pts de vie max = Taille + Constitution / 2 */
on("change:taille change:constitution_value", function(ev) {
    setPtsVieMax();
});

/* Pts d'endurance max = Taille + Const OU Pts de vie + Volonté */
on("change:taille change:constitution_value change:volonte_value", function(ev) {
    setPtsEnduranceMax();
});

/* Seuil de Constitution */
on("change:constitution_value", function(ev) {
    setSeuilConstitution();
});

/* Seuil de sustentation */
on("change:taille", function(ev) {
    setSeuilSustentation();
});

/* Bonus aux dommages */
on("change:taille change:force_value", function(ev) {
    setBonusDommages();
});

/* Seuil d'encombrement */
on("change:taille change:force_value", function(ev) {
    setSeuilEncombrement();
});


/* Fatigue max */
on("change:pts_endurance_max", function(ev) {
  setFatigueMax();
});

/* Fatigue */
on("change:pts_fatigue", function(ev) {
  setFatigueCases();
});

/* Fatigue bouton +1 */
on("clicked:fatigue_plus", function(ev) {
  fatiguePlus(1);
});

/* Fatigue bouton -1 */
on("clicked:fatigue_minus", function(ev) {
  fatigueMinus(1);
});

/* Fatigue bouton -H */
on("clicked:fatigue_minus_heure", function(ev) {
  getAttrs(['pts_fatigue',
    'fatigue_segment_01_max', 'fatigue_segment_02_max', 'fatigue_segment_03_max',
    'fatigue_segment_04_max', 'fatigue_segment_05_max', 'fatigue_segment_06_max',
    'fatigue_segment_07_max', 'fatigue_segment_08_max', 'fatigue_segment_09_max',
    'fatigue_segment_10_max', 'fatigue_segment_11_max', 'fatigue_segment_12_max'],
  function(res) {
    var ptsFatigue = parseInt(res.pts_fatigue);
    if (ptsFatigue > 0) {
      var segmax = [];
      segmax[0] = parseInt(res.fatigue_segment_01_max);
      segmax[1] = parseInt(res.fatigue_segment_02_max);
      segmax[2] = parseInt(res.fatigue_segment_03_max);
      segmax[3] = parseInt(res.fatigue_segment_04_max);
      segmax[4] = parseInt(res.fatigue_segment_05_max);
      segmax[5] = parseInt(res.fatigue_segment_06_max);
      segmax[6] = parseInt(res.fatigue_segment_07_max);
      segmax[7] = parseInt(res.fatigue_segment_08_max);
      segmax[8] = parseInt(res.fatigue_segment_09_max);
      segmax[9] = parseInt(res.fatigue_segment_10_max);
      segmax[10] = parseInt(res.fatigue_segment_11_max);
      segmax[11] = parseInt(res.fatigue_segment_12_max);
      var i, j, num, cnt = 0, do_break = 0;
      for (i = 0; i < 12; i++) {
        for (j = 0; j < 8; j++) {
          if (j < segmax[i]) {
            if (++cnt == ptsFatigue) {
              if (j < (segmax[i] / 2) - 1) {
                num = i > 0 ? segmax[i - 1] + j + 1 : j + 1;
              }
              else {
                num = j + 1;
              }
              do_break = 1;
            }
          }
          if (do_break) {
            break;
          }
        }
        if (do_break) {
          break;
        }
      }
      fatigueMinus(num);
    }
  });
});

/* Endurance bouton +1 */
on("clicked:endurance_plus", function(ev) {
  getAttrs(['pts_endurance', 'pts_endurance_max', 'pts_vie', 'pts_vie_max',],
  function(res) {
    var ptsEndurance = parseInt(res.pts_endurance);
    var enduranceMax = parseInt(res.pts_endurance_max);
    if (++ptsEndurance <= enduranceMax) {
      var pv = parseInt(res.pts_vie);
      var pvmax = parseInt(res.pts_vie_max);
      var ptsPerdus = pvmax - pv;
      var sustMalus = parseInt(res.faim_malus) + parseInt(res.soif_malus);
      if (ptsEndurance <= enduranceMax - (ptsPerdus * 2) ) {
        setAttrs({pts_endurance: ptsEndurance});
      }
    }
  });
});

/* Endurance bouton -1 */
on("clicked:endurance_minus", function(ev) {
  enduranceMinus(1);
});

/* Pts de vie bouton +1 */
on("clicked:vie_plus", function(ev) {
  getAttrs(['pts_vie', 'pts_vie_max'], function(res) {
    var pv = parseInt(res.pts_vie);
    var pvmax = parseInt(res.pts_vie_max);
    if (++pv <= pvmax) {
      var statut = '';
      if (pv <= 0) {
        statut = 'Coma!';
      }
      setAttrs({pts_vie: pv, vie_statut: statut, pts_vie_perdus: -(pvmax - pv),
        pts_vie_perdus_exp: Math.floor((pvmax - pv) / 2)});
    }
  });
});

/* Pts de vie bouton -1 */
on("clicked:vie_minus", function(ev) {
  vieMinus(1);
  });



</script>















